{
  "name": "discord.js-handler",
  "description": "Lightweight event and command handler for a discord.js project",
  "version": "1.1.4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start:dev": "tsnd -r tsconfig-paths/register src",
    "start:update": "run-s get-updates apply-updates",
    "start:build": "run-s clean compile run",
    "clean": "rimraf lib",
    "compile": "tsc",
    "run": "node -r tsconfig-paths/register -r ts-node/register lib",
    "get-updates": "ncu -u",
    "apply-updates": "npm i"
  },
  "author": "el3triK_",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "npm-check-updates": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "discord.js": "^11.5.1"
  }
}
